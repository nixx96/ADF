				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "West US",
						"nodeSize": "Standard_D8_v3",
						"numberOfNodes": 2,
						"maxParallelExecutionsPerNode": 8,
						"vNetProperties": {
							"vNetId": "/subscriptions/1387c4c3-4257-46f8-b8b8-ef22247254fe/resourceGroups/xenos/providers/Microsoft.Network/virtualNetworks/SSISIRVnet",
